<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gov.pbc.xjcloud.provider.contract.mapper.auditManage.AuditPlanInfoMapper">


    <select id="selectAuditPlanInfoList" parameterType="int" resultMap="AuditPlanInfoMap">
        select * from audit_plan_info a, plan_check_list b where a.plan_id=b.id and a.role_id='1'
    </select>
    <!-- 使用resultMap映射实体类和字段之间的一一对应关系 -->
    <resultMap type="gov.pbc.xjcloud.provider.contract.entity.auditManage.AuditPlanInfo" id="AuditPlanInfoMap">
        <id property="id" column="id"/>
        <result property="status" column="status"/>
        <result property="roleId" column="role_id"/>
        <association property="planCheckList" javaType="gov.pbc.xjcloud.provider.contract.entity.PlanCheckList">
            <id property="id" column="id"/>
            <result property="projectCode" column="project_code"/>
            <result property="projectName" column="project_name"/>
        </association>
    </resultMap>


</mapper>